{
  "name": "<%= project.name %>",
  "description": "<%= project.description %>",
  "main": "index.html",
  "license": [
    {
      "type": "<%= project.licenseType %>",
      "url": "<%= project.licenseUrl %>"
    }
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "keywords": "<%= project.keywords %>",
  "authors": [
    {
      "name": "<%= project.maintainerName %>"
    }
  ],
  "repository": {
    "type": "<%= project.repositoryType %>",
    "url": "<%= project.repositoryUrl %>"
  },
  "dependencies": {
    "domReady": "git://github.com/requirejs/domReady.git#~2.0.1",
    "requirejs": "~2.1.14",
    "requirejs-plugins": "~1.0.2",
    "handlebars": "~2.0.0"
  },
  "private": true
}
